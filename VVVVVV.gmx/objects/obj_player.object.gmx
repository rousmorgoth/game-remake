            <kind>1</kind>
            <string>Key_Left    = keyboard_check_direct(vk_left);
Key_Right   = keyboard_check_direct(vk_right);
Key_Jump    = keyboard_check_pressed(vk_up);

if (Key_Left)
{
    hspeed -= 1;
    image_xscale = -1;
    image_speed = 0.09;
}

if (Key_Right)
{
    hspeed += 1;
    image_xscale = 1;
    image_speed = 0.09;
}

if (hspeed &gt; 4) hspeed = 4;
if (hspeed &lt; -4) hspeed = -4;


if ((Key_Right &amp;&amp; Key_Left) or (!Key_Right &amp;&amp; !Key_Left))
{
    image_speed = 0;
}

// Obrácení gravitace
if (Key_Jump)
{
    // Pouze na zemi
    if (grounded)
    {
        if (position)
        {
            // strop
            position = 0;
            image_yscale = 1;
        } else {
            // zem
            position = 1;
            image_yscale = -1;
        } 
    }
}

// Gravitace - max rychlost pádu 4
if (position)
{
    // strop
    vspeed -= grav;
    if (vspeed &lt; -4) vspeed = -4;
} else {
    // zem
    vspeed += grav
    if (vspeed &gt; 4) vspeed = 4;
}

// Vertikální kolize
if place_meeting(x,y+vspeed,par_wall)
{
    while (!place_meeting(x,y+sign(vspeed),par_wall)) y += sign(vspeed);
    vspeed = 0;
    grounded = 1;
} else {
    grounded = 0;
}

// Horizontální kolize
if place_meeting(x+hspeed,y,par_wall)
{
    while (!place_meeting(x+sign(hspeed),y,par_wall)) x += sign(hspeed);
    hspeed = 0;
}

// move_step(hspeed,vspeed);

// Room loop
if (loop)
{
    if (x &gt; view_xview[0]+320) x = view_xview[0];
    if (x &lt; view_xview[0]) x = view_xview[0]+320;
    if (y &gt; view_yview[0]+240) y = view_yview[0];
    if (y &lt; view_yview[0]) y = view_yview[0]+240;
} else {
    if (x &gt; view_xview[0]+320) view_xview[0] += 320;
    if (x &lt; view_xview[0]) view_xview[0] -= 320;
    if (y &gt; view_yview[0]+240) view_yview[0] += 240;
    if (y &lt; view_yview[0]) view_yview[0] -= 240;
}

// Lasery
if (flip)
{
    if (position)
    {
        // strop
        position = 0;
        image_yscale = 1;
    } else {
        // zem
        position = 1;
        image_yscale = -1;
    }
    flip = 0;    
}

// Debugging
if (debug)
{
    if keyboard_check_pressed(vk_backspace) game_restart();
    if keyboard_check_pressed(vk_escape) game_end();
    if keyboard_check_pressed(vk_alt)
    {
        if (window_get_fullscreen() = true) window_set_fullscreen(false);
        else
        window_set_fullscreen(true);
    }
    if keyboard_check_pressed(vk_enter) name = 1;
    if (name = 1)
    {
        draw_set_color(c_orange);
        draw_rectangle(view_xport[0],view_yport[0]+232,view_xport[0]+320,view_xport[0]+240,false)
    }
}
</string>
